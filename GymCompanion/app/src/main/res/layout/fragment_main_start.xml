<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

    <TextView
            android:text="Home"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView5" android:layout_centerInParent="true"/>
    <Button
            android:text="Go to Progress"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btnGotoProgress" android:layout_centerHorizontal="true"
            android:layout_below="@+id/textView5" android:onClick="goToProgress"/>


</RelativeLayout>
